<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="actor" class="com.spring.stackroute.Actor">


        <property name="name" value="SRK"></property>
<!--        setter injection-->
        <property name="gender" value="Male"></property>
        <property name="age" value="50"></property>


    </bean>

    <bean id="actor" class="com.spring.stackroute.Movie">

        <property name="actor" reference="actor"></property>



    </bean>
    <!--    <bean id="VodafoneId" class="package com.spring.stackroute.Vodafone"></bean>-->

</beans>